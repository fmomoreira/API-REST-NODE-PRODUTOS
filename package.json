    "express": "^4.18.2",
    "express-rate-limit": "^6.7.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.1",
    "@types/swagger-ui-express": "^4.1.3",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
